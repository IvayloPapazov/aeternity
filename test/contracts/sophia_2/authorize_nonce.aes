contract GA =
  record state = { nonce : int, checks : int }

  function init() = { nonce = 1, checks = 0 }

  stateful function nonce_correct(n : int) : bool =
    if(state.nonce != n) abort("Incorrect nonce")
    put(state{nonce = n + 1})
    true

  stateful function check_nonce(n : int) : bool =
    put(state{checks = state.checks + 1})
    state.nonce == n

  function n_checks() : int =
    state.checks

